{
  "name": "calendar",
  "author": "Cameron Vargas",
  "version": "0.1.0",
  "description": "Django-React project for generating and a calendar and scheduler for public witnessing stands",
  "license": "ISC",
  "repository": "calendar",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "eslint": "eslint src/**/*.js --ignore-pattern node_modules/",
    "sasslint": "sass-lint -v -q",
    "htmlhint": "htmlhint",
    "lint": "npm run eslint & npm run sasslint & npm run htmlhint",
    "build": "npm run clean && webpack --config ./webpack/webpack.prod.js",
    "builddev": "npm run clean && webpack --config ./webpack/webpack.dev.js",
    "buildwatch": "npm run clean && webpack --progress --colors --watch --config ./webpack/webpack.prod.js",
    "serve": "webpack-dev-server --config ./webpack/webpack.dev.js",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "jest --verbose --coverage",
    "test:watch": "npm run test -- --watch",
    "webpack": "webpack --progress"
  },
  "jest": {
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "./setupTests.js"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.2.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-minify": "^0.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.10",
    "detect-browser": "^2.4.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^21.2.1",
    "jsdom": "^8.1.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.1",
    "react-addons-test-utils": "^0.14.7",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.3.0",
    "sass-loader": "^7.0.1",
    "sinon": "^4.0.0",
    "sass-lint": "^1.12.1",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack": "^3.11.0",
    "webpack-bundle-tracker": "^0.2.1",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-preset-stage-1": "^6.1.18",
    "bootstrap": "^4.0.0",
    "enzyme": "^3.1.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "moment": "^2.22.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-big-calendar": "^0.19.2",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-dom": "^16.0.0",
    "react-redux": "^4.4.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
